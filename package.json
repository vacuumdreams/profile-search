{
  "name": "bpbp",
  "version": "0.1.0",
  "description": "Prezi test project",
  "main": "api/dist/index.js",
  "engines": {
    "node": "^6.9.0"
  },
  "scripts": {
    "build-api": "cd api && ./node_modules/.bin/tsc",
    "build-app": "./node_modules/.bin/gulp",
    "lint-api": "cd api && ./node_modules/.bin/eslint --config .eslintrc src/**/*.js --fix",
    "test-unit-api": "cd api && ./node_modules/.bin/mocha dist/**/__unit/*.js",
    "test-integration-api": "cd api && ./node_modules/.bin/mocha dist/**/__integration/*.js",
    "start": "node index.js",
    "dev-build-api": "cd api && ./node_modules/.bin/tsc --watch",
    "dev-api": "NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js index.js",
    "dev-app": "NODE_ENV=development ./node_modules/.bin/gulp"
  },
  "author": "Botond Fekete",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "form-serialize": "^0.7.1",
    "normalize-scss": "^5.0.3",
    "ramda": "^0.23.0",
    "restify": "^4.3.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^6.0.57",
    "@types/ramda": "0.0.2",
    "@types/restify": "^2.0.36",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.7",
    "superagent": "^3.3.2",
    "typescript": "^2.1.4",
    "vinyl-named": "^1.1.0",
    "webpack": "^2.2.0-rc.7",
    "webpack-stream": "^3.2.0"
  }
}
