{
  "name": "bpbp-api",
  "version": "0.1.0",
  "description": "API for Prezi test project",
  "main": "dist/index.js",
  "engines": {
    "node": "^6.4.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/eslint --config .eslintrc src/**/*.js --fix",
    "test-unit": "./node_modules/.bin/mocha dist/**/__unit/*.js",
    "test-integration": "./node_modules/.bin/mocha dist/**/__integration/*.js",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\""
  },
  "author": "Botond Fekete",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "ramda": "^0.23.0",
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.57",
    "@types/ramda": "0.0.2",
    "@types/restify": "^2.0.36",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.7",
    "superagent": "^3.3.2",
    "typescript": "^2.1.4"
  }
}
